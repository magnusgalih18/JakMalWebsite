* 
    margin 0px


.navbar_container 
    background white
    display flex


nav 
    margin auto
    display flex
    align-items center
    border-radius 25px
    padding 20px
    background #fffae6
    width 500px
    align-items center
    justify-content center


nav ul 
    display flex
    list-style none


nav ul li 
    padding 5px


.navcontent_container 
    display flex
    flex-direction row
    justify-content space-between
    padding 5px
    align-items center


.number_nav 
    background-color #ff8a00
    height 25px
    width 25px
    border-radius 50%
    display flex
    align-items center
    justify-content center
    color white
    margin-right 5px


.text_nav 
    margin-right 15px
    color #ff8a00


.arrow_nav 
    margin-right 5px
    color #ff8a00


#nav3 
    background-color #ffe4b8
    color #ff8a00


body 
    background-color #fffae6
    margin-top 50px


.body_container 
    background-color white
    margin 0px 50px 0px 50px
    box-shadow 0px 4px 3px #eeeeee
    width fit-content
    height auto
    display flex
    flex-direction column


.page_container 
    display flex


.Shipment_Details 
    margin-left 35px


.previous_delivpage 
    text-decoration none
    display inline-block
    padding 8px 16px
    color black


.Shipment_Container 
    padding 40px 30px
    width 65%



.SD_upper_container 
    display flex
    justify-content space-between
    align-items center


.SD_body_title 
    color #ff8a00
    font-size x-large
    font-weight bolder
    text-shadow 5px 2px #eeeeee


.SD_Body 
    margin-top 15px
    display flex
    flex-direction row


.SD_Body_Shipping 
    width 100%
    display flex
    flex-direction row
    flex-wrap wrap


.PD_Body_Payment
    width 100%
    display flex
    flex-direction row
    flex-wrap wrap

.shipping_btn
    display flex
    justify-content center
    align-items center
    width 25%
    height 50px
    background-color white
    border 2px solid grey 
    border-radius 0px
    margin-right 10px


.payment_btn
    display flex
    justify-content center
    align-items center
    width 25%
    height 50px
    background-color white
    border 2px solid grey 
    border-radius 0px
    margin-right 10px



.Payment_Container 
    padding 40px 30px
    display flex
    flex-direction column


.PD_upper_container 
    display flex
    justify-content space-between
    align-items center


.PD_body_title 
    color #ff8a00
    font-size x-large
    font-weight bolder
    text-shadow 5px 2px #eeeeee


.PD_Body 
    margin-top 15px
    display flex
    flex-direction row


.PD_Body_Payment 
    width 100%
    display flex
    flex-direction row
    flex-wrap wrap


.DDForm1 
    display contents

.Summary 
    display flex
    padding 40px 30px
    flex-direction column
    justify-content space-between
    width 35%


.Summary_upper_container 
    flex-direction column
    margin-bottom 25px


.Sm_Body_title 
    color #ff8a00
    font-size large
    font-weight bolder
    margin-bottom 10px


.Summary_deliv_container
    display flex
    flex-direction column
    margin-bottom 100px


.sm_deliv_detail
    display flex
    flex-direction row
    color #41df91 
    font-weight bold


.Sm_deliv_title
    color black
    font-weight bold


.total_item 
    color grey



.CostOfGoods_container 
    color grey
    display flex
    flex-direction row
    justify-content space-between
    margin-bottom 15px


.cog_value 
    color black
    font-weight bold


.Dropshipping_fee_container 
    color grey
    display none
    flex-direction row
    justify-content space-between
    font-size medium
    margin-bottom 15px


.df_value 
    color black
    font-weight bold


.Shipment_fee_container 
    color grey
    display flex
    flex-direction row
    justify-content space-between
    font-size medium
    margin-bottom 15px


.Shipment_value 
    color black


.total_price_container 
    color grey
    display flex
    flex-direction row
    justify-content space-between
    margin-bottom 15px


.tp_text 
    color #ff8a00
    font-size larger
    font-weight bolder


.tp_value 
    color #ff8a00
    font-size larger
    font-weight bolder


button
    width 100%
    padding 20px
    color white
    font-size large
    background-color #ff8a00
    box-shadow 0px 4px 3px grey


@media screen and (max-width1023px)
    .page_container 
        display flex
        flex-direction column
        align-items center
    
    .DD_Body
        display flex
        flex-direction column
        align-items center
    
    .DD_Body_User 
        width 80%
        margin-right 0px
    
    .DD_Body_Dropshiper
        width 80%
    
    .SD_upper_container
        justify-content center
    
    .PD_upper_container
        justify-content center
    
  
